<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.blin.myapplication.IntentTest">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="显示网页"
        android:id="@+id/INBT1"
         />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="显示地图"
        android:id="@+id/INBT2"
        android:layout_above="@+id/INBT4"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="路径规划"
        android:id="@+id/INBT3"
        android:layout_alignTop="@+id/INBT2"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打电话"
        android:id="@+id/INBT4"
        android:layout_marginTop="54dp"
        android:layout_below="@+id/INBT1"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="传送SMS/MMS"
        android:id="@+id/INBT5"
        android:layout_alignTop="@+id/INBT4"
        android:layout_alignEnd="@+id/INBT2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="传送 Email"
        android:id="@+id/INBT6"
        android:layout_alignTop="@+id/INBT5"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Uninstall"
        android:id="@+id/INBT7"
        android:layout_below="@+id/INBT4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="52dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="播放多媒体"
        android:id="@+id/INBT8"
        android:layout_alignTop="@+id/INBT7"
        android:layout_alignStart="@+id/INBT5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Market 相关 "
        android:id="@+id/INBT9"
        android:layout_alignTop="@+id/INBT8"
        android:layout_alignStart="@+id/INBT3" />

</RelativeLayout>
